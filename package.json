  "version": "1.2",
  "private": true,
  "scripts": {
    "lint": "next lint",
    "lint:es": "eslint --ext .js,.jsx .",
    "lint:fix": "eslint --fix --ext .js,.jsx .",
    "dev": "env-cmd -f .env.local next dev",
    "build": "env-cmd -f .env.local next build",
    "start": "env-cmd -f .env.local next start",
    "build:dev": "env-cmd -f .env.development next build",
    "start:dev": "env-cmd -f .env.development next start",
    "build:prod": "env-cmd -f .env.production next build",
    "start:prod": "env-cmd -f .env.production next start",
    "export": "env-cmd -f .env.local yarn build && next export -o _static",
    "export:dev": "env-cmd -f .env.development yarn build && next export -o _static",
    "export:prod": "env-cmd -f .env.production yarn build && next export -o _static",
    "clean": "npx rimraf .next out"
  },
